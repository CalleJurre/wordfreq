
def tokenize(lines):
    words = []
    for line in lines:
        start = 0

        while start < len(line):
            if not line[start].isspace():
                
                if line[start].isdigit():
                    print(f'{line[start]} is a digit')
                elif line[start].isalpha():
                    print(f'{line[start]} is a letter')
            start += 1
    return words


lines = ['  2  sw   4   10  eet  apple  tart Ã¤r gott 1 2 3']
tokenize(lines)
